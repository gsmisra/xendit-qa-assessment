    "chai": "^4.2.0",
    "cypress": "^5.1.0",
    "cypress-cucumber-preprocessor": "^2.5.5",
    "cypress-iframe": "^1.0.1",
    "cypress-image-snapshot": "^3.1.1",
    "cypress-promise": "^1.1.0",
    "cypress-xpath": "^1.6.0",
    "electron": "^9.2.1",
    "express": "^4.17.1",
    "mocha": "^8.1.1",
    "mochawesome": "^6.1.1",
    "named-regexp": "^0.1.1",
    "prettier": "^2.0.5",
    "save-dev": "0.0.1-security",
    "sharp": "^0.26.0",
